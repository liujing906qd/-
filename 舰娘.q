Function Ken_GetTimeStrByTimestamp(Timestamp)
    Dim Hour,Minute,Second
    Hour = Int(Timestamp / 3600)
    Minute = Int((Timestamp Mod 3600) / 60)
    Second = Int(Timestamp Mod 3600) Mod 60
    Ken_GetTimeStrByTimestamp = Hour & ":" & Minute & ":" & Second
    Call Ken_Logcat_D("Ken_GetTimeStrByTimestamp:" & Ken_GetTimeStrByTimestamp & " By Timestamp:" &Timestamp)
End Function

Function Ken_WriteLog(Log)
    Call Plugin.File.WriteFileEx(DebugLogPath, Log)
    TracePrint Log
End Function
Function Ken_WriteInputBox(Log)
    Dim FileLenSave,FileLenNow
    
    LogLine = LogLine + 1
    If (LogLine > 1000) Then 
        Call Ken_LogInputBoxClear()
    End If
    
    FileLenSave = Plugin.File.GetFileLength(DebugLogPath)
    Call Ken_WriteLog(Log)
    FileLenNow = Plugin.File.GetFileLength(DebugLogPath)
    
    HwndFile = Plugin.File.OpenFile(DebugLogPath)
    Call Plugin.File.SeekFile(HwndFile,FileLenSave)
        
    LogcatFileStr = Plugin.File.ReadFile(HwndFile, FileLenNow - FileLenSave)
    Form.IB_Logcat.Text = LogcatFileStr & Form.IB_Logcat.Text

    Plugin.File.CloseFile (HwndFile)
    FileLenSave = FileLenNow
End Function
Function Ken_LogInputBoxClear()
    Form.IB_Logcat.Text = ""
    LogcatFileStr = ""
    LogLine = 0
End Function

/**ÈÕÖ¾**/
Function Ken_Logcat_D(Log)
    If (Form.OB_Logcat_Debug.Value = 1) Then 
        Call Ken_WriteInputBox(Log)
    Else 
        Call Ken_WriteLog(Log)
    End If
End Function
Function Ken_Logcat_I(Log)
    Call Ken_WriteInputBox(Log)
End Function
Function Ken_Logcat_W(Log)
    Call Ken_Logcat_I("*******************************"&Log&"*******************************")
End Function
Function Ken_Logcat_E(Log)
    Call Ken_Logcat_I(Log)
    
    If (Form.CB_Logcat_WhenWaringSavePicture.Value = 1) Then 
        Call Plugin.BGCP2_02.SavePicture(HwndTidu,0,0,800,1000, "./ÈÕÖ¾/" & Ken_GetDateTimeStr() &"_"& Log &".png")
    End If
End Function

