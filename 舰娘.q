Function FactoryCountClick(Number,Position,isDev)
    Dim Ge, Shi, Bai
    Dim Decrease
    If (isDev) Then 
        Decrease = 10
    Else 
        Decrease = 30
    End If
    
    Number = Number - Decrease
    
    Bai = Int(Number / 100)
    Shi = Int(Number / 10 Mod 10)
    Ge = Int(Number Mod 10)
    Call Ken_Logcat_D("Bai" & Bai & " Shi" & Shi & " Ge" & Ge & " Number" & Number & " Position" & Position)
	
    Dim Ken_LeftClickOnXY_Delay_Temp
    Ken_LeftClickOnXY_Delay_Temp = Ken_LeftClickOnXY_Delay
    Ken_LeftClickOnXY_Delay = 100
    Dim indexForBai,indexForShi,indexForGe
    For indexForBai = 0 To Bai - 1 Step 1
        Call Ken_PointAddAndClick(Position, 0, 30)
    Next
    For indexForShi = 0 To Shi - 1 Step 1
        Call Ken_PointAddAndClick(Position, 0, 0)
    Next
    For indexForGe = 0 To Ge - 1 Step 1
        Call Ken_PointAddAndClick(Position, -127, 17)
    Next
    Ken_LeftClickOnXY_Delay = Ken_LeftClickOnXY_Delay_Temp
End Function


