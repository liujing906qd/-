/**********************************************************函数**********************************************************/
Function GoToMuGang()
    Dim WayFromExpeditionFinish
    Call SetHwndByGame()
    Call Ken_Logcat_I("尝试返回母港")
    //回到母港
    If (Ken_FindPic("母港_出击") = False)
        If (Ken_FindPicAndClick("左侧UI_母港")) Then 
        	Call Ken_Logcat_I("返回母港")
            Call Ken_DeleyForNet()
        Else 
            Call Ken_Logcat_E("无法回到母港")
        End If
    End If
    
    Do While(Ken_FindPicAndClick("远征_结束_开始"))
        Call Ken_Logcat_I("发现远征结束舰队")
        Call Ken_DeleyForNet()
        Delay 7000
        If (Ken_FindPicUntilFoundThenClick("远征_结束_评价") = False) Then 
            Call Ken_Logcat_E("接收远征舰队失败，无法找到评价。尝试直接接收")
            Call Ken_FindPicUntilFoundThenClick("远征_结束_评价_继续")
        End If
        Delay 500
        Call Ken_FindPicUntilFoundThenClick("远征_结束_评价_继续")
        Delay 1000
        Call Ken_DeleyForNet()
        WayFromExpeditionFinish = True
    Loop
    If (WayFromExpeditionFinish) Then 
        WayFromExpeditionFinish = False
        Call SupplyNavy()
    End If
End Function
Function GoToScene(SceneName)
    Call SetHwndByGame()
    
    Call Ken_Logcat_W("即将前往" & SceneName)
    If (SceneName = "远征") Then 
        Call GoToMuGang()
        If (Ken_FindPicUntilFoundThenClick("母港_出击") = False) Then
            Call Ken_Logcat_E("远征失败，没有找到出击按钮，返回母港")
            Call GoToMuGang()
            Exit Function
        End If
        Call Ken_DeleyForNet()
        Call Ken_FindPicUntilFoundThenClick("出击_主界面_远征")
        Call Ken_DeleyForNet()
        Exit Function
    End If
    
    If (Ken_FindPicAndClick("左侧UI_" & SceneName) = False) Then 
        Call GoToMuGang()
        If (Ken_FindPicUntilFoundThenClick("母港_" & SceneName) = False) Then 
        Call Ken_Logcat_W("警告：无法前往" & SceneName & "可能导致程序逻辑异常")
        Call Ken_Logcat_W("警告：无法前往" & SceneName & "可能导致程序逻辑异常")
        Call Ken_Logcat_W("警告：无法前往" & SceneName & "可能导致程序逻辑异常")
        Call Ken_Logcat_W("如果出现异常，请尝试停止脚本-回到母港-重启脚本")
        Exit Function
        End If
    End If
    Call Ken_Logcat_W("进入" & SceneName & "界面")
    Call Ken_DeleyForNet()
End Function

/*远征主逻辑*/

//开始远征
Function ExpeditionNavy()
    Call Ken_Logcat_I("远征模块是否打开:" & Form.CB_AutoExpedition.Value)
    Call SetHwndByGame()
    If (Form.CB_AutoExpedition.Value = 0) Then 
        Exit Function
    End If
    
    Call GoToScene("远征")
    
    If (Expedition_Navy2_Enable = 1) Then 
        Call ExpeditionSelectMapByMissionID(ExpeditionMissionID_Navy2,1)	
    End If
    If (Expedition_Navy3_Enable = 1) Then 
