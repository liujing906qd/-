End Function
Function ExpeditionNavySetClock()
    ExpeditionSetUIAndTimer (2)
    ExpeditionSetUIAndTimer (3)
    ExpeditionSetUIAndTimer (4)
End Function
Function ExpeditionSetUIAndTimer(NavyNumber)
    Dim NavyPic
    If (NavyNumber = 2) Then 
        NavyPic = "远征_列表_第二舰队"
    ElseIf (NavyNumber = 3) Then
        NavyPic = "远征_列表_第三舰队"
    Else 
        NavyPic = "远征_列表_第四舰队"
    End If

    If (Ken_FindPicAndClick(NavyPic)) Then 
        Dim Hour_Shi, Hour_Ge, Minute_Shi, Minute_Ge, Second_Shi, Second_Ge
        Dim HourNumber, MinuteNumber, SecondNumber
        Hour_Shi = Int(ExpeditionGetTimeNumberByRect(3, Expedition_Time_HourX)) * 10
        Hour_Ge = Int(ExpeditionGetTimeNumberByRect(3, Expedition_Time_HourX + 9))
		
        Minute_Shi = ExpeditionGetTimeNumberByRect(2, Expedition_Time_MinuteX) * 10
        Minute_Ge = ExpeditionGetTimeNumberByRect(2, Expedition_Time_MinuteX + 9)
		
        Second_Shi = ExpeditionGetTimeNumberByRect(1, Expedition_Time_SecondX) * 10
        Second_Ge = ExpeditionGetTimeNumberByRect(1, Expedition_Time_SecondX + 9)
		
        HourNumber = Hour_Shi + Hour_Ge
        MinuteNumber = Minute_Shi + Minute_Ge
        SecondNumber = Second_Shi + Second_Ge
       
        Dim Timestamp 
        Timestamp = Ken_HourNumberToSecondNumber(HourNumber) + Ken_MinuteNumberToSecondNumber(MinuteNumber) + SecondNumber
        If (NavyNumber = 2) Then 
            Form.Timer_Expedition_UpdateStr_Navy_No2.Enabled = False
            ExpeditionTimestampNavyNo2 = Timestamp
            Form.CB_Expedition_Time_Navy2.Caption = Ken_GetTimeStrByTimestamp(ExpeditionTimestampNavyNo2)
            Form.Timer_Expedition_UpdateStr_Navy_No2.Enabled = True
            Form.Timer_Expedition_UpdateStr_Navy_No2.Interval = 1000
            
            Call Ken_Logcat_I("第二舰队远征剩余秒数:" & ExpeditionTimestampNavyNo2)
        ElseIf (NavyNumber = 3) Then
            Form.Timer_Expedition_UpdateStr_Navy_No3.Enabled = False
            ExpeditionTimestampNavyNo3 = Timestamp
            Form.CB_Expedition_Time_Navy3.Caption = Ken_GetTimeStrByTimestamp(ExpeditionTimestampNavyNo3)
            Form.Timer_Expedition_UpdateStr_Navy_No3.Enabled = True
            Form.Timer_Expedition_UpdateStr_Navy_No3.Interval = 1000
            
            Call Ken_Logcat_I("第三舰队远征剩余秒数:" & ExpeditionTimestampNavyNo3)
        Else 
            Form.Timer_Expedition_UpdateStr_Navy_No4.Enabled = False
            ExpeditionTimestampNavyNo4 = Timestamp
            Form.CB_Expedition_Time_Navy4.Caption = Ken_GetTimeStrByTimestamp(ExpeditionTimestampNavyNo4)
            Form.Timer_Expedition_UpdateStr_Navy_No4.Enabled = True
            Form.Timer_Expedition_UpdateStr_Navy_No4.Interval = 1000
            
            Call Ken_Logcat_I("第四舰队远征剩余秒数:" & ExpeditionTimestampNavyNo4)
        End If
        
    End If
End Function
/*解析远征时间的区域，获得具体时间*/
Function ExpeditionGetTimeNumberByRect(TypeNumber, Time_X)
    Dim StartX, StartY, EndX, EndY
    StartX = Time_X
    StartY = Expedition_Time_All_Y
    EndX = StartX + 9//8
    EndY = StartY + 13//13
	
    Dim TypeStr
    If (TypeNumber = 3) Then 
        TypeStr = "小时"
    ElseIf (TypeNumber = 2) Then
        TypeStr = "分钟"
    Else 
        TypeStr = "秒数"
    End If

    ExpeditionGetTimeNumberByRect = -1
    For TimeIndex = 9 To 0 Step -1
        If (Ken_FindPicByRectAndDefinition("远征_时间_" & TypeStr & "_" & TimeIndex, StartX, StartY, EndX, EndY, 0.8)) Then 
            ExpeditionGetTimeNumberByRect = TimeIndex
            Exit Function
        End If
    Next
    If (ExpeditionGetTimeNumberByRect) Then 
        Call Ken_Logcat_W("获取远征时间时出错，如果经常出现该问题，请打开“日志-出错时保存图片”并与作者Ken联系")
    End If
End Function

Function CheckOilAndAmmoIsFull()
	Call Ken_Logcat_W("正在检查当前舰队是否缺油缺弹药")
    If (Ken_FindPic("远征_结束_舰队_缺油")) Then 
        CheckOilAndAmmoIsFull = False
        Exit Function
    End If
    
    If (Ken_FindPic("远征_结束_舰队_极度缺油")) Then 
        CheckOilAndAmmoIsFull = False
