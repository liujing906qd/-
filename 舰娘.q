//工厂开发
Function FactoryNavy()
    Dim FactoryNumberArray
    Call Ken_Logcat_I("工厂模块是否打开:" & Form.CB_AutoFactory.Value)
    If (Form.CB_AutoFactory.Value = 0) Then 
        Exit Function
    End If
    Call GoToScene("工厂")
    Delay 1000
    Call FactoryDev()
    Call FactoryBuild()
    If (Int(Form.IB_Dev_RepeatCount.Text) <= 0 And Int(Form.IB_Build_RepeatCount.Text) <= 0) Then 
        Form.CB_AutoFactory.Value = 0
        Call Ken_Logcat_W("开发和建造次数都已经到0，为了效率，工厂模块自动关闭")
    End If
End Function

Function FactoryDev()
    Call Ken_Logcat_D("Int(Form.IB_Dev_RepeatCount.Text):" & Int(Form.IB_Dev_RepeatCount.Text))
    Call Ken_Logcat_D("IsNumeric(Form.IB_Dev_RepeatCount.Text):" & IsNumeric(Form.IB_Dev_RepeatCount.Text))
    While (Int(Form.IB_Dev_RepeatCount.Text) > 0)
        If (IsNumeric(Form.IB_Dev_RepeatCount.Text) = False)
            Call Ken_Logcat_W("开发次数文本框内容为空，或者不是数字")
            Exit Function
        End If

        Call Ken_FindPicUntilFoundThenClick("工厂_主界面_开发")
        Delay 200 //防卡
        If (Ken_FindPicUntilFound("工厂_开发_开发界面") = False) Then 
            Call Ken_Logcat_W("没有成功打开开发界面，退出开发，回到母港")
            Call GoToMuGang()
            Exit Function
        End If
   	
        FactoryNumberArray = Split(Int(Form.IB_Dev_Oil.Text) & "|" & Int(Form.IB_Dev_Steel.Text) & "|" & Int(Form.IB_Dev_Ammo.Text) & "|" & Int(Form.IB_Dev_Lv.Text), "|")
   	
        Dim mIndex
        For mIndex = 0 To 3 Step 1
            Call FactoryCountClick(FactoryNumberArray(mIndex),FactoryPositionArray(mIndex),True)
        Next
    
        Call Ken_FindPicUntilFoundThenClick("工厂_开发_开发开始")
        Call Ken_DeleyForNet()
        Delay 5000
        Call Ken_FindPicUntilFoundThenClick("出击_结束_评价_返回")
        
        Form.IB_Dev_RepeatCount.Text = Int(Form.IB_Dev_RepeatCount.Text) - 1
    Wend
    Call Ken_Logcat_I("开发次数小于等于0，开发结束")
End Function
Function FactoryBuild()
    Call Ken_Logcat_D("Int(Form.IB_Build_RepeatCount.Text):" & Int(Form.IB_Build_RepeatCount.Text))
    While (Int(Form.IB_Build_RepeatCount.Text) > 0)
        If (IsNumeric(Form.IB_Build_RepeatCount.Text) = False)
            Call Ken_Logcat_W("建造次数文本框内容为空，或者不是数字")
            Exit Function
        End If
        
        If (Ken_FindPicAndClick("工厂_建造_建造完毕")) Then 
            Call Ken_Logcat_I("发现建造完毕的舰船，正在获取")
            Call Ken_DeleyForNet()
            Delay 5000
            Call Ken_FindPicUntilFoundThenClick("出击_结束_评价_返回")
        End If
        
        Call Ken_FindPicUntilFoundThenClick("工厂_建造_选择空位")
        Delay 200 //防卡
        If (Ken_FindPicUntilFound("工厂_建造_建造界面") = False) Then 
            Call Ken_Logcat_W("没有成功打开建造界面，退出建造，回到母港")
            Call GoToMuGang()
            Exit Function
        End If
   	
        FactoryNumberArray = Split(Int(Form.IB_Build_Oil.Text) & "|" & Int(Form.IB_Build_Steel.Text) & "|" & Int(Form.IB_Build_Ammo.Text) & "|" & Int(Form.IB_Build_Lv.Text), "|")
   	
        Dim mIndex
        For mIndex = 0 To 3 Step 1
            Call FactoryCountClick(FactoryNumberArray(mIndex),FactoryPositionArray(mIndex),False)
        Next
    
        Call Ken_FindPicUntilFoundThenClick("工厂_建造_建造开始")
        Call Ken_DeleyForNet()
        Form.IB_Build_RepeatCount.Text = Int(Form.IB_Build_RepeatCount.Text) - 1
    Wend
    Call Ken_Logcat_I("建造次数小于等于0，建造结束")
End Function

Function FactoryCountClick(Number,Position,isDev)
    Dim Ge, Shi, Bai
    Dim Decrease
    If (isDev) Then 
        Decrease = 10
    Else 
        Decrease = 30
    End If
    
    Number = Number - Decrease
    
    Bai = Int(Number / 100)
    Shi = Int(Number / 10 Mod 10)
    Ge = Int(Number Mod 10)
    Call Ken_Logcat_D("Bai" & Bai & " Shi" & Shi & " Ge" & Ge & " Number" & Number & " Position" & Position)
	
    Dim Ken_LeftClickOnXY_Delay_Temp
    Ken_LeftClickOnXY_Delay_Temp = Ken_LeftClickOnXY_Delay
    Ken_LeftClickOnXY_Delay = 100
    Dim indexForBai,indexForShi,indexForGe
    For indexForBai = 0 To Bai - 1 Step 1
        Call Ken_PointAddAndClick(Position, 0, 30)
    Next
    For indexForShi = 0 To Shi - 1 Step 1
        Call Ken_PointAddAndClick(Position, 0, 0)
    Next
    For indexForGe = 0 To Ge - 1 Step 1
        Call Ken_PointAddAndClick(Position, -127, 17)
    Next
    Ken_LeftClickOnXY_Delay = Ken_LeftClickOnXY_Delay_Temp
End Function


Function FacotryDissolve()
    Call GoToScene("工厂")
    Call Ken_Logcat_W("解体舰船")
    Call Ken_FindPicUntilFoundThenClick("工厂_主界面_解体")
    If (Ken_ClickUntilChangeTo(FactoryDissolveType, "工厂_解体_列表类型_新入手")) Then 
        Dim DissolveIsFinish
        Do
            DissolveIsFinish = False
            Call Ken_PointGetAndClick(FactoryDissolveFirstShipPosition)
            If (Ken_FindPicAndClick("工厂_解体_确定解体")) Then 
                DissolveIsFinish =True
                Call Ken_DeleyForNet()
                Delay 3000
            End If
        Loop While(DissolveIsFinish)
    End If
    
End Function
























































