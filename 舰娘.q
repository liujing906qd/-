End Event
Event Form.LoadOver
    LB_Dev_Setting = Plugin.File.ReadINI("Factory", "DevList", ".\Config.ini")
    LB_Dev_SettingArray = Split(LB_Dev_Setting, "|")
    If (UBound(LB_Dev_SettingArray) = -1) Then 
        Form.LB_Dev.AddItem "10,10,10,10"
    End If
    For i = 0 To UBound(LB_Dev_SettingArray)
        Item = LB_Dev_SettingArray(i) 
        Form.LB_Dev.AddItem Item
    Next
    LB_Build_Setting = Plugin.File.ReadINI("Factory", "BuildList", ".\Config.ini")
    LB_Build_SettingArray = Split(LB_Build_Setting, "|")
    If (UBound(LB_Build_SettingArray) = -1) Then 
        Form.LB_Build.AddItem "30,30,30,30"
    End If
    For i = 0 To UBound(LB_Build_SettingArray)
        Item = LB_Build_SettingArray(i)
        Form.LB_Build.AddItem Item
    Next
    Form.IB_Dev_RepeatCount.Text = 0
    Form.IB_Build_RepeatCount.Text = 0
    Form.CB_AutoFactory.Value = 0
End Event
