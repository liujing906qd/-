            Exit Function
        End If
    End If
    Call Ken_FindPicUntilFoundThenClick("出击_远征_远征开始")
    Call Ken_DeleyForNet()
    Delay 5000
End Function
/*获取远征时间*/
Function ExpeditionNavyIsTimeisup()
