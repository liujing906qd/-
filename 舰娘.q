/**查找图片，直到超时或者图片位置点击**/
Function Ken_FindPicUntilFoundThenClick(picName)
    Ken_FindPicUntilFoundThenClick = False
    If (Ken_FindPicUntilFound(picName)) Then 
        Ken_FindPicUntilFoundThenClick = Ken_LeftClickOnXY()
    End If
End Function

/**查找图片，直到超时**/
Function Ken_FindPicUntilFound(picName)
    Dim Timeout
    Timeout = 0
    Do
        Delay 100
        Timeout = Timeout + 100
        If (Timeout >= 5000) Then 
            Ken_FindPicUntilFound = False
            Exit Function
        End If
    Loop While (Ken_FindPic(picName) = False)
    Ken_FindPicUntilFound = True
End Function

/**点击XY的位置**/
Function Ken_LeftClickOnXY()
    If X <= - 1  Then 
        Ken_LeftClickOnXY = False
        Exit Function
    End If
    Call Ken_Logcat_D("Ken_LeftClickOnXY x:" & X & " Y :" & Y)
    If (Y < 800) Then     	
        Call Plugin.Bkgnd.LeftClick(Hwnd, X, Y)
        Ken_LeftClickOnXY = True
    Else 
        Call Plugin.Bkgnd.LeftClick(HwndTidu, X, Y)
        Ken_LeftClickOnXY = True
    End If
    X = -1 
    Y = - 1 
    Delay Ken_LeftClickOnXY_Delay
End Function

/**存储坐标给某个变量**/
Function Ken_PointSet(positionX, positionY)
    Ken_PointSet = positionX & "|" & positionY
End Function

/**获得坐标从某个变量**/
Function Ken_PointGet(XY)
    Dim ZuoBiao
    ZuoBiao = InStr(XY, "|")
    X = Clng(Left(XY, ZuoBiao - 1)) : Y = Clng(Right(XY, Len(XY) - ZuoBiao))
End Function

/**获得坐标从某个变量，然后单击**/
Function Ken_PointGetAndClick(XY)
    Ken_PointGet (XY)
    
    Call Ken_LeftClickOnXY()
End Function

/**某个变量与坐标相加**/
Function Ken_PointAdd(XY,positionX,positionY)
    Call Ken_PointGet(XY)
    X = X + positionX
    Y = Y + positionY
End Function
/**某个变量与坐标相加，然后单击**/
Function Ken_PointAddAndClick(XY,positionX,positionY)
    Call Ken_PointAdd(XY,positionX,positionY)
    Call Ken_LeftClickOnXY()
End Function

/**点击坐标，直到出现图片**/
Function Ken_ClickUntilChangeTo(Position,PicName)
    Dim Timeout
    Timeout = 0
    Do While (Ken_FindPic(PicName) = False)
        Call Ken_PointGetAndClick(Position)
        Delay 100
        Timeout = Timeout + 100
        If (Timeout >= 5000) Then 
            Ken_ClickUntilChangeTo = False
            Exit Function
        End If
    Loop
    Ken_ClickUntilChangeTo = True
End Function

Function Ken_DeleyForNet()
    Call Ken_Logcat_D("网络等待")
    Dim FindTime, nCount
    FindTime = Plugin.Sys.GetTime()
    Do
        If (Ken_FindPicByRectAndDefinition("全局_载入中", 770, 450, 800, 480, 0.7)) Then 
            FindTime = Plugin.Sys.GetTime()
        End If
    Loop While (FindTime  + 1500 > Plugin.Sys.GetTime())
    Call Ken_Logcat_D("获取网络数据完毕")
End Function

Function Ken_ErrorEndScript()
    Call Ken_Logcat_E("出现了严重错误，脚本即将中止。可尝试重启脚本、游戏和电脑。如问题继续请联系作者")
    EndScript
End Function

//时间函数
Function Ken_GetDateTimeStr()
    Ken_GetDateTimeStr = Year(now) & "_" & Month(now) & "_" & Day(now) & "_" & Hour(now) & "_" & Minute(now) & "_" & Second(now)
End Function
/**时间转换**/
Function Ken_HourNumberToSecondNumber(HourNumber)
    Ken_HourNumberToSecondNumber = Ken_MinuteNumberToSecondNumber(HourNumber * 60)
End Function
Function Ken_MinuteNumberToSecondNumber(MinuteNumber)
    Ken_MinuteNumberToSecondNumber = MinuteNumber * 60
End Function
Function Ken_GetTimeStrByTimestamp(Timestamp)
    Dim Hour,Minute,Second
    Hour = Int(Timestamp / 3600)
    Minute = Int((Timestamp Mod 3600) / 60)
    Second = Int(Timestamp Mod 3600) Mod 60
    Ken_GetTimeStrByTimestamp = Hour & ":" & Minute & ":" & Second
    Call Ken_Logcat_D("Ken_GetTimeStrByTimestamp:" & Ken_GetTimeStrByTimestamp & " By Timestamp:" &Timestamp)
End Function

Function Ken_WriteLog(Log)
    Call Plugin.File.WriteFileEx(DebugLogPath, Log)
    TracePrint Log
End Function
Function Ken_WriteInputBox(Log)
    Dim FileLenSave,FileLenNow
    
    LogLine = LogLine + 1
    If (LogLine > 1000) Then 
        Call Ken_LogInputBoxClear()
    End If
    
    FileLenSave = Plugin.File.GetFileLength(DebugLogPath)
    Call Ken_WriteLog(Log)
    FileLenNow = Plugin.File.GetFileLength(DebugLogPath)
    
    HwndFile = Plugin.File.OpenFile(DebugLogPath)
    Call Plugin.File.SeekFile(HwndFile,FileLenSave)
        
    LogcatFileStr = Plugin.File.ReadFile(HwndFile, FileLenNow - FileLenSave)
    Form.IB_Logcat.Text = LogcatFileStr & Form.IB_Logcat.Text

    Plugin.File.CloseFile (HwndFile)
    FileLenSave = FileLenNow
End Function
Function Ken_LogInputBoxClear()
    Form.IB_Logcat.Text = ""
    LogcatFileStr = ""
    LogLine = 0
End Function

/**日志**/
Function Ken_Logcat_D(Log)
    If (Form.OB_Logcat_Debug.Value = 1) Then 
        Call Ken_WriteInputBox(Log)
    Else 
        Call Ken_WriteLog(Log)
    End If
End Function
Function Ken_Logcat_I(Log)
    Call Ken_WriteInputBox(Log)
End Function
Function Ken_Logcat_W(Log)
    Call Ken_Logcat_I("*******************************"&Log&"*******************************")
End Function
Function Ken_Logcat_E(Log)
    Call Ken_Logcat_I(Log)
    
    If (Form.CB_Logcat_WhenWaringSavePicture.Value = 1) Then 
        Call Plugin.BGCP2_02.SavePicture(HwndTidu,0,0,800,1000, "./日志/" & Ken_GetDateTimeStr() &"_"& Log &".png")
    End If
End Function

/**对比颜色**/
Function Ken_CmpColor(mColor,PositionX,PositionY)
    Ken_CmpColor = Plugin.BGCP2_02.CmpColor(Hwnd, PositionX, PositionY, mColor, 0)
    Call Ken_Logcat_D("Ken_CmpColor Ken_CmpColor:" & Ken_CmpColor & "PositionX:" & PositionX & "     PositionY:" & PositionY & " mColor:" & mColor & " Hwnd:" & Hwnd)
End Function
Function Ken_GetColorByPosition(PositionX,PositionY)
    Ken_GetColorByPosition = Plugin.BGCP2_02.GetPixelColor(Hwnd, PositionX, PositionY)
    Call Ken_Logcat_D("Ken_GetColorByPosition Ken_GetColorByPosition:" & Ken_GetColorByPosition & "	    PositionX:" & PositionX & " PositionY:" & PositionY & " Hwnd:" & Hwnd)
End Function
Function Ken_FindColor(mColor)
    Ken_FindColor = Ken_FindColorByRect(mColor, 0, 0, 800, 480)
End Function
Function Ken_FindColorByRectAndColorOffset(mColor, mStartX, mStartY, mEndX, mEndY, ColorOffset)
    Ken_FindColorByRectAndColorOffset = Plugin.BGCP2_02.FindColor(Hwnd, mStartX, mStartY, mEndX, mEndY, mColor, ColorOffset, 0, ColorX, ColorY)
    
    If (ColorX > - 1 ) Then 
        Ken_FindColorByRectAndColorOffset = True
    Else 
        Ken_FindColorByRectAndColorOffset = False
    End If
    
    Call Ken_Logcat_D("Ken_FindColorByRectAndColorOffset ColorX:" & ColorX & " ColorY:" & ColorY & "     mStartX:" & mStartX  & " mStartY:" & mStartY   & " mEndX:" & mEndX  & " mEndY:" & mEndY & " ColorOffset:" & ColorOffset & " Hwnd:" & Hwnd)
End Function
Function Ken_FindColorByRect(mColor, mStartX, mStartY, mEndX, mEndY)
    Ken_FindColorByRect = Ken_FindColorByRectAndColorOffset(mColor, mStartX, mStartY, mEndX, mEndY, 0)
End Function