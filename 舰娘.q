Function Ken_FindPic(picName)
    Call Lib_FindPic(picName)
    If X > - 1  Then //找到了
        Call Ken_Logcat_D("Ken_FindPic:" & picName & " FoundPic = true")
        Ken_FindPic = True
    Else //没知道，可能是被选中的状态，更换选中状态的图片再找一次
        Call Lib_FindPic(picName & "_touched")
        If X > - 1  Then 
            Ken_FindPic = True//找到了	
        Else 
            Call Ken_Logcat_D("Ken_FindPic:" & picName & " FoundPic = false")
            Ken_FindPic = False//真没找到
        End If
    End If
End Function
