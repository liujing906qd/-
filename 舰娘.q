Function TD_Expedition()
    Call SetHwndByGame()
    Call GoToMuGang()
    If (Form.CB_Supply_When_Expedition_Before.Value = 1) Then 
        Call SupplyNavy()
    End If
    Call ExpeditionNavy()
End Function

/****************************出击****************************/
Function TD_Attack()
    Call SetHwndByGame()
    Call GoToMuGang()
    If (Form.CB_Supply_When_Attack_Before.Value = 1) Then 
        Call SupplyNavy()
    End If
    
    If (ChangeNavy()) Then 
        Call Ken_Logcat_I("舰船状态检测完毕，准备出击")
        Call AttackNavy()
    Else 
        Call Ken_Logcat_W("舰船状态检查，无法出击")
    End If
    Call GoToMuGang()
    If (Form.CB_Attack_After_Repair.Value = 1) Then 
        Call RepairNavy()
    End If
End Function


