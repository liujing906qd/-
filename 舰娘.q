        Call Ken_PointAddAndClick(AttackMissionPositionOfSpecialMission, 0, Form.CB_Attack_MissionLevel.ListIndex * AttackMissionPositionOfSpecialMissionOffset)
        Call Ken_PointAddAndClick(AttackMissionPositionOfSpecialMission, 0, Form.CB_Attack_MissionLevel.ListIndex * AttackMissionPositionOfSpecialMissionOffset)
        Call Ken_PointAddAndClick(AttackMissionPositionOfSpecialMission, 0, Form.CB_Attack_MissionLevel.ListIndex * AttackMissionPositionOfSpecialMissionOffset)
    End If
	
    Delay 1000//防卡
    If (Ken_FindPic("出击_确定任务_舰船到达上限")) Then 
        Call Ken_Logcat_W("舰船数量到达上限")
        If (Form.CB_Attack_Dissolve.Value = 1) Then 
    		Call Ken_Logcat_I("按照用户设置，准备解体舰船")
            Call FacotryDissolve()
        End If
    End If
	
    
    Call Ken_FindPicUntilFoundThenClick("出击_确定任务")
    Delay 500//防卡
    
    If (Form.CB_Attack_When_DaPo.Value = 1) Then 
    	If (Ken_FindPic("出击_红脸_大破") Or Ken_FindPic("出击_黄脸_大破") Or Ken_FindPic("出击_正常和闪_大破")) Then 
    		Call Ken_Logcat_W("出击检测：有船大破，停止出击！")
    		Exit Function
    	End If
    End If
    If (Form.CB_Supply_CheckOil.Value = 1) Then 
        If (CheckOilAndAmmoIsFull() = false) Then 
            Call Ken_Logcat_W(NavyNumber & "舰队在执行任务" & ExpeditionMissionID + 1  & "时缺油或弹药")
            Call SupplyNavy()
            Exit Function
        End If
    End If
    
    If (Ken_FindPic("出击_出击_出击开始_disable") = False) Then 
        Call Ken_FindPicUntilFoundThenClick("出击_出击_出击开始")
        Call Fighting()
        Call Ken_DeleyForNet()
    End If
End Function
//战斗中
Function Fighting()
	Dim Timeout, FightingRound
    FightingRound = 0
    Timeout = 0
    
    Call Ken_Logcat_W("出击开始")
    Do
        Call Ken_FindPicAndClick("战斗_战斗中_指南针")
        
        Call Ken_PointGetAndClick(AttackFormationPositionArray(Form.CB_Attack_Formation.ListIndex))
        
        If (AttackNightFight = 1) Then 
            Call Ken_FindPicAndClick("战斗_战斗中_脱离判定_夜战")
        Else 
            Call Ken_FindPicAndClick("战斗_战斗中_脱离判定_追击")
        End If
        If (FightingRound < AttackFightRound) Then 
            If (Ken_FindPicAndClick("战斗_战斗中_回合结束_进击")) Then 
                FightingRound = FightingRound + 1
                Call Ken_Logcat_I("当前战斗回合数：" & FightingRound & " 总攻击回合数：" & AttackFightRound)
                Delay 1000
            End If
        Else 
            Call Ken_FindPicAndClick("战斗_战斗中_回合结束_撤退")
        End If
        If (Ken_FindPicAndClick("战斗_结束_评价_战果报告")) Then 
            Call Ken_Logcat_I("战斗结束，判断舰船是否大破")
            If (Ken_FindPicByRect("战斗_战斗中_战斗状态_大破", 0, 0, 400, 480)) Then 
                Call Ken_Logcat_I("检测到舰船大破，不再继续进击")
                ShipWasDestroyed = 1
                FightingRound = 999//不在继续战斗
            End If
        End If
        Call Ken_FindPicAndClick("进击_结束_评价_继续")
        Call Ken_FindPicAndClick("出击_结束_评价_返回")
        Delay 1000
        Timeout = Timeout + 1000
        
        FightOutTime = Int(Form.IB_Setting_Fight_OutTime.Text)
        If (FightOutTime <= 0) Then 
            FightOutTime = 10
        End If
        
        If (Timeout >= FightOutTime * 60 * 1000) Then 
            Exit Function
        End If
    Loop While (Ken_FindPic("母港_出击") = False)
    //Call Ken_FindPicUntilFoundThenClick("出击_结束_评价_继续")
End Function

//换船
Function ChangeNavy()
    Dim ChangeNavyNeedChangePortIndex, ChangeNavyDontNeedChangeNumber
    ChangeNavyDontNeedChangeNumber = 0
    
    Call Ken_Logcat_I("编成模块是否打开" & Form.CB_AutoChangeNavy.Value)
    If (Form.CB_AutoChangeNavy.Value = 0) Then 
        Call Ken_Logcat_W("按照用户设置，编成模块没有打开")
        ChangeNavy = True//没有更换，则认为已按照用户设定完成更换
        Exit Function
    End If
    Call GoToScene("编成")
    
    Call Ken_Logcat_W("获取并更新当前舰船数量")
    ShipCount = ChangeNavyGetShipCountInCurrentNavy()
    Form.ChangeNavy_ShipCount.Caption = ShipCount
    
    Call Ken_Logcat_W("检查哪艘船需要被替换")
    For ChangeNavyNeedChangePortIndex = 0 To ShipCount
        Call Ken_Logcat_I("开始检测第" & ChangeNavyNeedChangePortIndex+1 & "船位的舰船状态")
        MultNumX = Int(ChangeNavyNeedChangePortIndex / 2)
        MultNumY = ChangeNavyNeedChangePortIndex Mod 2
        If (ChangeNavyGetStateByColorRect(MultNumX, MultNumY)) Then //舰船当前状态是否需要更换其他舰船
            Call Ken_Logcat_I("第" & ChangeNavyNeedChangePortIndex+1 & "船位需要更换舰船！")
            If (ChangeNavyChangeInit()) Then 
                Call Ken_Logcat_I("第" & ChangeNavyNeedChangePortIndex+1 & "船位更换成功")
                ChangeNavy = True
            Else
                Call Ken_Logcat_W("按照用户设置，现在需要更换舰船，但是更换失败")
                ChangeNavy = False
                Exit Function
            End If
        Else 
            Call Ken_Logcat_I("第" & ChangeNavyNeedChangePortIndex + 1 & "船位不需要更换")
            ChangeNavyDontNeedChangeNumber = ChangeNavyDontNeedChangeNumber + 1
        End If
    Next
    
    Call Ken_DeleyForNet()
    
    ChangeNavy = True
End Function
//获得当前舰队的舰船数量
Function ChangeNavyGetShipCountInCurrentNavy()
    Dim ShipCount
    Dim mStartX, mStartY, mEndX, mEndY
    Dim MultNumX,MultNumY
    
    ShipCount = 0
    
    For CardIndex = 0 To 5
        Call Ken_Logcat_I("开始检测第" & CardIndex+1 & "船位是否有船")
        MultNumX = Int(CardIndex / 2)
        MultNumY = CardIndex Mod 2
        
        mStartX = ChangeNavyShipCardPositionX + ChangeNavyHPPositionX * (MultNumX)
        mStartY = ChangeNavyShipCardPositionY + ChangeNavyHPPositionY * (MultNumY)
        mEndX = mStartX + 245
        mEndY = mStartY + 105
        If (Ken_FindPicByRect("编成_列表_空位", mStartX, mStartY, mEndX, mEndY)) Then 
            Call Ken_Logcat_I("没有找到第" & CardIndex+1 & "艘船，已经找到舰船数量为：" & ShipCount)
            Exit Function
        Else 
            ShipCount = ShipCount + 1
        End If
        ChangeNavyGetShipCountInCurrentNavy = ShipCount
    Next
    If (ShipCount <> 6) Then 
        Call Ken_Logcat_W("只获取到" & ShipCount & "艘船，")
        Call Ken_Logcat_W("请确认舰船数量是否正常，")
        Call Ken_Logcat_W("如果数量正确请无视这段提醒，")
        Call Ken_Logcat_W("否则请立即中止该脚本运行")
    End If
End Function

//获取是否需要更换这个坐标区域的舰船
Function ChangeNavyGetStateByColorRect(MultNumX,MultNumY)
    Dim mStartX, mStartY, mEndX, mEndY
    mStartX = 210 + ChangeNavyHPPositionX * (MultNumX)
    mStartY = 170 + ChangeNavyHPPositionY * (MultNumY)
    mEndX = mStartX + 65
    mEndY = mStartY + 7
	
    If (Form.CB_ChangeNavy_When_Repair.Value = 1) Then 
        If (Ken_CmpColor(BGR_Ship_Repair, mStartX + 210, mStartY - 14)) Then 
            ChangeNavyGetStateByColorRect = True
            Exit Function
        End If
    End If
    If (Form.CB_ChangeNavy_When_Condition_0_20.Value = 1) Then 
        If (Ken_CmpColor(BGR_Ship_State_HongLian, mStartX + 239, mStartY)) Then 
            ChangeNavyGetStateByColorRect = True
            Exit Function
        End If
    End If
    If (Form.CB_ChangeNavy_When_Condition_20_30.Value = 1) Then 
        If (Ken_CmpColor(BGR_Ship_State_HuangLian, mStartX + 239, mStartY)) Then 
            ChangeNavyGetStateByColorRect = True
            Exit Function
        End If
    End If
    If (Form.CB_ChangeNavy_When_DaPo.Value = 1) Then 
        If (Ken_FindColorByRect(BGR_Ship_HP_DaPo, mStartX, mStartY, mEndX,mEndY)) Then 
            ChangeNavyGetStateByColorRect = True
            Exit Function
        End If
    End If
    If (Form.CB_ChangeNavy_When_ZhongPo.Value = 1) Then 
        If (Ken_FindColorByRect(BGR_Ship_HP_ZhongPo, mStartX, mStartY, mEndX,mEndY)) Then 
            ChangeNavyGetStateByColorRect = True
            Exit Function
        End If
    End If
    If (Form.CB_ChangeNavy_When_XiaoPo.Value = 1) Then 
        If (Ken_FindColorByRect(BGR_Ship_HP_XiaoPo, mStartX, mStartY, mEndX,mEndY)) Then 
            ChangeNavyGetStateByColorRect = True
            Exit Function
        End If
    End If
    If (Ken_FindColorByRectAndColorOffset(BGR_Ship_HP_Wusun, mStartX, mStartY, mEndX ,mEndY ,30)) Then //获取绿色血条。这个判断只为了判断是否获取到血条颜色，避免程序出现问题无法去色时出击
        ChangeNavyGetStateByColorRect = False
        Exit Function
    End If
End Function
//开始更换
Function ChangeNavyChangeInit()
    //通过船位，获得用户定义的下一艘可替换舰船
    Call Ken_Logcat_I("分析舰船文本框数据，即将开始更换")
    Call Ken_Logcat_D("ChangeNavyNeedChangePortIndex:" & ChangeNavyNeedChangePortIndex)
    
    Dim ChangeNavyIDs
    //获得当前（ChangeNavyNeedChangePortIndex）船位上的所有ID
    If (ChangeNavyNeedChangePortIndex = 0) Then 
        ChangeNavyPlaceIndex = Form.IB_ChangeNavy1.Text
        ChangeNavyIDs = Int(Form.Label_ChangeNavy_1.Caption)
    ElseIf (ChangeNavyNeedChangePortIndex = 1) Then
        ChangeNavyPlaceIndex = Form.IB_ChangeNavy2.Text
        ChangeNavyIDs = Int(Form.Label_ChangeNavy_2.Caption)
    ElseIf (ChangeNavyNeedChangePortIndex = 2) Then
        ChangeNavyPlaceIndex = Form.IB_ChangeNavy3.Text
        ChangeNavyIDs = Int(Form.Label_ChangeNavy_3.Caption)
    ElseIf (ChangeNavyNeedChangePortIndex = 3) Then
        ChangeNavyPlaceIndex = Form.IB_ChangeNavy4.Text
        ChangeNavyIDs = Int(Form.Label_ChangeNavy_4.Caption)
    ElseIf (ChangeNavyNeedChangePortIndex = 4) Then
        ChangeNavyPlaceIndex = Form.IB_ChangeNavy5.Text
        ChangeNavyIDs = Int(Form.Label_ChangeNavy_5.Caption)
    ElseIf (ChangeNavyNeedChangePortIndex = 5) Then 
        ChangeNavyPlaceIndex = Form.IB_ChangeNavy6.Text
        ChangeNavyIDs = Int(Form.Label_ChangeNavy_6.Caption)
    Else 
        ChangeNavyPlaceIndex = ""
        ChangeNavyIDs = ""
    End If
    Call Ken_Logcat_I("ChangeNavyIDs:" & ChangeNavyIDs &"ChangeNavyPlaceIndex:" & ChangeNavyPlaceIndex)
    If (ChangeNavyPlaceIndex = "") Then 
        Call Ken_Logcat_W("替换船位是第" & ChangeNavyNeedChangePortIndex + 1 & "号位置的舰船：没有填写可替换的舰船，跳出替换舰船模块")
        ChangeNavyChangeInit = False
        Exit Function
    Else 
        Call Ken_Logcat_I("替换船位是第" & ChangeNavyNeedChangePortIndex + 1 & "号位置的舰船：已获取数据成功")
        ChangeNavyChangeInitResult = 0
    End If
    
    //通过船位获得它的当前使用的舰船ID数组
    ChangeNavyIDArray = Split(ChangeNavyPlaceIndex, "|", - 1 , 1)
    //当前要更换的舰船ID
    ShipID = ChangeNavyIDArray(ChangeNavyIDs)//通过舰船ID数组和下标获得当前要更换的舰船ID
    
    If (IsNumeric(ShipID) = False) Then 
        Call Ken_Logcat_W("分析第" & ChangeNavyNeedChangePortIndex + 1 & "号位置的舰船数据：填写的" & ShipID & "不是数字，跳出替换舰船模块")
        ChangeNavyChangeInit = False
        Exit Function
    Else
        Call Ken_Logcat_I("分析第" & ChangeNavyNeedChangePortIndex + 1 & "号位置的舰船数据：成功解析，填写的" & ShipID & "")
    End If
    If (ChangeNavyChanging()) Then 
        ChangeNavyChangeInit = True
    Else 
        ChangeNavyChangeInit = False
        Exit Function
    End If
End Function
Function ChangeNavyChanging()
    Dim PageCount,LineIndex//舰船ID所在的页数和行数
    PageCount = Int(ShipID  / 10)
    LineIndex = Int(ShipID Mod 10)
    
    Call Ken_Logcat_W("将更换第" & LineIndex &"页的第" & PageCount &"行船，ShipID:" & ShipID)
    //换船页数
    ChangeNavyPageCount = pageCount
    //换船行数
    ChangeNavyLineNumber = LineIndex
    
    Call Ken_Logcat_I("分析完毕，符合更换条件，开始更换")
    Dim ChangeNavyUpdateResult
    Call Ken_Logcat_I("点击更换按钮")
    Call Ken_PointAddAndClick(ChangeNavyChangeInitBtnPosition, ChangeNavyChangeInitBtnPositionOffsetX * (ChangeNavyNeedChangePortIndex Mod 2), ChangeNavyChangeInitBtnPositionOffsetY * Int(ChangeNavyNeedChangePortIndex / 2))
    Delay 200//防卡
    
    If (Ken_FindPicUntilFound("编成_列表_顶部条") = False) Then 
        Call Ken_Logcat_I("没有找到列表的顶部条，可能没有点中更换按钮，更换失败，重新回到母港")
        Call GoToMuGang()
        ChangeNavyChanging = False
        Exit Function
    End If

    //选择可以替换的船
    Call Ken_Logcat_I("标准化列表类型")
    //列表类型
    Call Ken_ClickUntilChangeTo(TypePosition, "编成_列表_类型_船只")
    //初始化到最前页
    Call Ken_PointGetAndClick(ChangeNavyBackToFirstPagePosition)
    //直接选择第三页
    Call Ken_PointAddAndClick(ChangeNavyPagePosition, ChangeNavyPagePositionOffset * 2, 0)
    //按翻5页按钮
    For index = 0 To Int(ChangeNavyPageCount / 5) - 1
        Call Ken_PointGetAndClick(ChangeNavyJumpPageToNext5Position)
    Next
    //在（12345）中选按具体哪页按钮
    Call Ken_PointAddAndClick(ChangeNavyPagePosition, ChangeNavyPagePositionOffset * (ChangeNavyPageCount Mod 5), 0)
    Delay 3000
    
    Call Ken_Logcat_I("选择第" & ChangeNavyLineNumber & "行的舰船")
    Call Ken_PointAddAndClick(ChangeNavyListPosition, 0, ChangeNavyListPositionOffset * (ChangeNavyLineNumber))
    
    Call ChangeNavySetNextShipIndex(ChangeNavyNeedChangePortIndex)//设置下一个要更换的INDEX
    Delay 1000
    
    If (Ken_FindPic("编成_列表_确定变更_disable") Or ChangeNavyGetShipIsReady() = False) Then
        Call Ken_Logcat_W("无法更改当前舰船。可能是在正在使用（入渠、已在舰队内），跳过至下一个定义的舰船ID再更换")
        //从舰船详情回到列表
        Call Ken_PointGetAndClick(ChangeNavyListNavyPosition)
        Delay 200//防卡
        Call Ken_PointGetAndClick(ChangeNavyListNavyPosition)
        Call ChangeNavyChangeInit()//无法更改当前舰船。可能是在正在使用（入渠、已在舰队内），跳过至下一个定义的舰船ID再更换
        Exit Function
    Else 
        Call Ken_Logcat_I("确定变更，开始")
        Call Ken_FindPicUntilFoundThenClick("编成_列表_确定变更")
        Call Ken_DeleyForNet()
        Call Ken_Logcat_I("确定变更，结束")
        ChangeNavyChanging = True
    End If
End Function
//更换后，设置下一个要更换的INDEX
Function ChangeNavySetNextShipIndex(ChangeNavyNeedChangePortIndex)
    //如果当前舰船下标已经获取到最后一个，则从头重新开始
    If (ChangeNavyIDs + 1 > UBound(ChangeNavyIDArray)) Then 
        ChangeNavyIDs = 0
    Else 
        ChangeNavyIDs = ChangeNavyIDs + 1
    End If
    If (ChangeNavyNeedChangePortIndex = 0) Then 
        Form.Label_ChangeNavy_1.Caption = ChangeNavyIDs
    ElseIf (ChangeNavyNeedChangePortIndex = 1) Then
        Form.Label_ChangeNavy_2.Caption = ChangeNavyIDs
    ElseIf (ChangeNavyNeedChangePortIndex = 2) Then
        Form.Label_ChangeNavy_3.Caption = ChangeNavyIDs
    ElseIf (ChangeNavyNeedChangePortIndex = 3) Then
        Form.Label_ChangeNavy_4.Caption = ChangeNavyIDs
    ElseIf (ChangeNavyNeedChangePortIndex = 4) Then
        Form.Label_ChangeNavy_5.Caption = ChangeNavyIDs
    ElseIf (ChangeNavyNeedChangePortIndex = 5) Then 
        Form.Label_ChangeNavy_6.Caption = ChangeNavyIDs
    End If
    Call Ken_Logcat_D("ChangeNavyIDs:" & ChangeNavyIDs & "ChangeNavyPageCount:" & ChangeNavyPageCount & "ChangeNavyLineNumber:" & ChangeNavyLineNumber & "UBound(ChangeNavyIDArray):" & UBound(ChangeNavyIDArray))
End Function
